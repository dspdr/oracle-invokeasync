# OAuth (IDCS)
TOKEN_URL=https://<your-idcs-host>/oauth2/v1/token
CLIENT_ID=
CLIENT_SECRET=
SCOPE=urn:opc:resource:fusion:<POD>:fusion-ai/

# Fusion AI Orchestrator base (no trailing slash)
# Example: https://cptbsnpqy-test.fusionapps.ocs.oc-test.com
FUSION_BASE_URL=https://<your-fusion-host>

# Agent / Workflow
WORKFLOW_CODE=TI_USER_INPUT_VAR_TEST
INVOCATION_MODE=ADMIN

# Optional defaults
CONVERSATIONAL=true
VERIFY_SSL=true
